{
    "address": "$IPADDR",
    "port": 5200,
    "secret": "multipass",
    "timeout": 300,
    "config": {
        "kubeAdmAddress": "$MASTER_IP",
        "kubeAdmToken": "$TOKEN",
        "kubeAdmCACert": "sha256:$CACERT",
        "kubeAdmExtraArguments": [
            "--ignore-preflight-errors=All",
            "--kubernetes-version=$KUBERNETES_VERSION"
        ]
    }
}